import java.util.*;

public class Lab14_problem3
{
  public static void main(String[] args)
  {
    ArrayList<Int> ListNums = new ArrayList<Int>();
    
    fvPop(ListNums);
    //fvDisplay(State);
  }
  public static void fvPop(ArrayList<Int> ListNums)
  {
    int iRand;
    int iCount;
    Random rand = new Random();
    for(iCount=0; iCount<100; iCount++)
    {
    iRand = rand.nextInt((10-1)+1)+1;
    ListNums.add(iRand);
    }
  }
