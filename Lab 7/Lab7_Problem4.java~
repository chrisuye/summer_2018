import java.util.*;
public class Lab7_Problem4
{
  
  public static void main(String args[]) 
  {
    int iCol;
    int iRow;
    int iNum;
    iNum=0;
    for (iCol =0; iCol <6; iCol++)
    {
      for (iRow =0; iRow <4; iRow++)
      {
      System.out.println("/t"+iNum); 
      iNum = iNum + 3;
      }
    }
       
  }
}